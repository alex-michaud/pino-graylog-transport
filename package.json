{
  "name": "@alex-michaud/pino-graylog-transport",
  "version": "1.2.0",
  "author": "Alexandre Michaud <alex-michaud@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-michaud/pino-graylog-transport.git"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/bun": "1.3.5",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "@types/chai": "^5.2.3",
    "@types/k6": "^1.4.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.3",
    "chai": "6.2.2",
    "esbuild": "^0.27.2",
    "mitata": "^1.0.34",
    "mocha": "11.7.5",
    "pino": "10.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "winston": "^3.19.0",
    "winston-log2gelf": "^2.4.1",
    "winston-transport": "^4.9.0"
  },
  "bugs": {
    "url": "https://github.com/alex-michaud/pino-graylog-transport/issues"
  },
  "description": "Pino transport that sends log to graylog",
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/graylog-transport.js",
    "dist/graylog-transport.d.ts",
    "dist/gelf-formatter.js",
    "dist/gelf-formatter.d.ts",
    "dist/message-queue.js",
    "dist/message-queue.d.ts",
    "dist/socket-connection.js",
    "dist/socket-connection.d.ts",
    "dist/udp-client.js",
    "dist/udp-client.d.ts",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/alex-michaud/pino-graylog-transport#readme",
  "keywords": [
    "pino",
    "transport",
    "graylog",
    "logging",
    "gelf"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "npm run test:unit",
    "test:unit": "mocha -r ts-node/register 'test/unit/**/*.test.ts'",
    "test:integration": "EXECUTE_INTEGRATION=true mocha -r ts-node/register 'test/integration/**/*.test.ts'",
    "test:all": "mocha -r ts-node/register 'test/{unit,integration}/**/*.test.ts'",
    "test:load": "esbuild test/load/load-test.ts --bundle --outfile=dist/load-test.js --target=es2017 --format=esm --external:k6 --external:k6/* && (command -v k6 >/dev/null && k6 run dist/load-test.js || echo \"k6 not found. Install k6 to run load tests: https://k6.io/docs/getting-started/installation/\")",
    "test:smoke": "esbuild test/load/smoke-test.ts --bundle --outfile=dist/smoke-test.js --target=es2017 --format=esm --external:k6 --external:k6/* && (command -v k6 >/dev/null && k6 run dist/smoke-test.js || echo \"k6 not found. Install k6 to run load tests: https://k6.io/docs/getting-started/installation/\")",
    "benchmark": "ts-node test/benchmark/benchmark.ts",
    "benchmark:load": "npm run build && esbuild test/benchmark/comparison-load-test.ts --bundle --outfile=dist/comparison-load-test.js --target=es2017 --format=esm --external:k6 --external:k6/* --tree-shaking=false --log-level=info && k6 run dist/comparison-load-test.js",
    "start:comparison-server": "npm run build && ts-node test/benchmark/comparison-server.ts",
    "start:comparison-server:bun": "npm run build && bun run test/benchmark/comparison-server.ts",
    "start:load-server": "ts-node test/load/server.ts",
    "start:load-server:bun": "bun run test/load/server.ts",
    "lint": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "ci": "biome ci .",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:setup": "ts-node scripts/setup-graylog.ts"
  },
  "types": "dist/index.d.ts"
}
